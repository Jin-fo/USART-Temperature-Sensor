                                 ;***************************************************************************
                                 ;*
                                 ;* "mpy16u" - 16x16 Bit Unsigned Multiplication
                                 ;*
                                 ;* This subroutine multiplies the two 16-bit register variables
                                 ;* mp16uH:mp16uL and mc16uH:mc16uL.
                                 ;* The result is placed in m16u3:m16u2:m16u1:m16u0.
                                 ;*  
                                 ;* Number of words :14 + return
                                 ;* Number of cycles :153 + return
                                 ;* Low registers used :None
                                 ;* High registers used  :7 (mp16uL,mp16uH,mc16uL/m16u0,mc16uH/m16u1,m16u2,
                                 ;*                          m16u3,mcnt16u)
                                 ;*
                                 ;***************************************************************************
                                 ;***** Subroutine Register Variables
                                 .def mc16uL =r16 ;multiplicand low byte
                                 .def mc16uH =r17 ;multiplicand high byte
                                 .def mp16uL =r18 ;multiplier low byte
                                 .def mp16uH =r19 ;multiplier high byte
                                 
                                 .def m16u0 =r18 ;result byte 0 (LSB)
                                 .def m16u1 =r19 ;result byte 1
                                 .def m16u2 =r20 ;result byte 2
                                 .def m16u3 =r21 ;result byte 3 (MSB)
                                 .def mcnt16u =r22 ;loop counter
                                 ;***** Code
                                 mpy16u:
000118 2755                      	clr m16u3 ;clear 2 highest bytes of result
000119 2744                      	clr m16u2
00011a e160                      	ldi mcnt16u,16 ;init loop counter
00011b 9536                      	lsr mp16uH
00011c 9527                      	ror mp16uL
                                 
                                 m16u_1:
00011d f410                      	brcc noad8 ;if bit 0 of multiplier set
00011e 0f40                      	add m16u2,mc16uL ;add multiplicand Low to byte 2 of res
00011f 1f51                      	adc m16u3,mc16uH ;add multiplicand high to byte 3 of res
                                 noad8:
000120 9557                      	ror m16u3 ;shift right result byte 3
000121 9547                      	ror m16u2 ;rotate right result byte 2
000122 9537                      	ror m16u1 ;rotate result byte 1 and multiplier High
000123 9527                      	ror m16u0 ;rotate result byte 0 and multiplier Low
000124 956a                      	dec mcnt16u ;decrement loop counter
000125 f7b9                      	brne m16u_1 ;if not done, loop more
000126 9508                      	ret
                                 
                                 ;***************************************************************************
                                 ;*
                                 ;* "bin16_to_BCD" - 16-bit Binary to BCD Conversion
                                 ;*
                                 ;* Description: Converts a 16-bit unsigned binary number to a five digit
                                 ;* packed BCD number. Uses subroutine div16u from Atmel application note AVR200
                                 ;*
                                 ;* Author: Ken Short
                                 ;* Version: 0.0
                                 ;* Last updated: 111320
                                 ;* Target: ATmega4809 or AVR128DB48
                                 ;* Number of words:
                                 ;* Number of cycles:
                                 ;* Low registers modified: r14, r15
                                 ;* High registers modified: r16, r17, r18, r19, r20, r22, r23, r24
                                 ;*
                                 ;* Parameters: r17:r16 16-bit unsigned right justified number to be converted.
                                 ;* Returns: r24:r23:r22 five digit packed BCD result.
                                 ;*
                                 ;* Notes:
                                 ;* Subroutine uses repeated division by 10 to perform conversion.
                                 ;* Calls div16u from application note AVR200
                                 ;***************************************************************************
                                 bin16_to_BCD:
000127 e030                      	ldi r19, 0 ;high byte of divisor for div16u
000128 e02a                      	ldi r18, 10 ;low byte of the divisor for div16u
                                 
000129 d00c                      	rcall div16u ;divide original binary number by 10
00012a 2d6e                      	mov r22, r14 ;result is BCD digit 0 (least significant digit)
00012b d00a                      	rcall div16u ;divide result from first division by 10, gives digit 1
00012c 94e2                      	swap r14 ;swap digit 1 for packing
00012d 296e                      	or r22, r14 ;pack
                                 
00012e d007                      	rcall div16u ;divide result from second division by 10, gives digit 2
00012f 2d7e                      	mov r23, r14 ;place in r23
000130 d005                      	rcall div16u ;divide result from third division by 10, gives digit 3
000131 94e2                      	swap r14 ;swap digit 3 for packing
000132 297e                      	or r23, r14 ;pack
                                 
000133 d002                      	rcall div16u ;divide result from fourth division by 10, gives digit 4
000134 2d8e                      	mov r24, r14 ;place in r24
                                 
000135 9508                      ret
                                 ;Subroutine div16u is from Atmel application note AVR200
                                 ;***************************************************************************
                                 ;*
                                 ;* "div16u" - 16/16 Bit Unsigned Division
                                 ;*
                                 ;* This subroutine divides the two 16-bit numbers
                                 ;*# "dd16uH:dd16uL" (dividend) and "dv16uH:dv16uL" (divisor).
                                 ;* The result is placed in "dres16uH:dres16uL" and the remainder in
                                 ;* "drem16uH:drem16uL".
                                 ;*  
                                 ;* Number of words :19
                                 ;* Number of cycles :235/251 (Min/Max)
                                 ;* Low registers used :2 (drem16uL,drem16uH)
                                 ;* High registers used  :5 (dres16uL/dd16uL,dres16uH/dd16uH,dv16uL,dv16uH,
                                 ;*    dcnt16u)
                                 ;*
                                 ;***************************************************************************
                                 ;***** Subroutine Register Variables
                                 .def drem16uL=r14
                                 .def drem16uH=r15
                                 .def dres16uL=r16
                                 .def dres16uH=r17
                                 .def dd16uL =r16
                                 .def dd16uH =r17
                                 .def dv16uL =r18
                                 .def dv16uH =r19
                                 .def dcnt16u =r20
                                 ;***** Code
                                 div16u:
000136 24ee                      	clr drem16uL			;clear remainder Low byte
000137 18ff                      	sub drem16uH,drem16uH	;clear remainder High byte and carry
000138 e141                      	ldi dcnt16u,17			;init loop counter
                                 d16u_1:
000139 1f00                      	rol dd16uL				;shift left dividend
00013a 1f11                      	rol dd16uH
00013b 954a                      	dec dcnt16u				;decrement counter
00013c f409                      	brne d16u_2				;if done
00013d 9508                      	ret						;return
                                 d16u_2:
00013e 1cee                      	rol drem16uL			;shift dividend into remainder
00013f 1cff                      	rol drem16uH
000140 1ae2                      	sub drem16uL,dv16uL		;remainder = remainder - divisor
000141 0af3                      	sbc drem16uH,dv16uH		;
000142 f420                      	brcc d16u_3				;if result negative
000143 0ee2                      	add drem16uL,dv16uL		;restore remainder
000144 1ef3                      	adc drem16uH,dv16uH
000145 9488                      	clc						;clear carry to be shifted into result
000146 cff2                      	rjmp d16u_1				;else
000147 9408                      d16u_3: sec					;set carry to be shifted into result
000148 cff0                      	rjmp d16u_1
                                 
